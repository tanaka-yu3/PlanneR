<% users.each do |user| %>
  <div class="col-lg-3 responsibe-center" style="float: left;">
    <% if admin_signed_in? %>
      <%= link_to admins_user_path(user) do %>
        <%= attachment_image_tag user , :image, :fill, 200,200 ,fallback:"noimage.png",size:"200x200", class:"image-action" %><br>
        <div>
    	  <%= user.family_name %>さん
        </div>
      <% end %>
  </div>
    <% else %>
      <%= link_to user_path(user) do %>
        <%= attachment_image_tag user , :image, :fill, 200,200 ,fallback:"noimage.png",size:"200x200", class:"image-action" %><br>
        <div>
    	  <%= user.family_name %>さん
        </div>
  </div>
      <% end %>
    <% end %>
<% end %>